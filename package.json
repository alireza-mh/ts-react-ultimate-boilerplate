{
  "name": "Boilerplate",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean-dist && webpack -p --config=configs/webpack/prod.js",
    "clean-dist": "rm -f -r -d dist",
    "lint": "yarn run lint:ts && yarn run lint:sass",
    "lint:ts": "tslint './src/**/*.ts*' --format stylish --force",
    "lint:sass": "stylelint ./src/**/*.scss",
    "start": "yarn run start-dev",
    "start-dev": "webpack-dev-server --config=configs/webpack/dev.js",
    "start-prod": "yarn run build && node express.js",
    "test": "jest --watch --coverage --config=configs/jest.json",
    "profile": "webpack -p --config=configs/webpack/prod.js --profile --json > webpack-stats.json",
    "extract": "nodejs bin/extract.js && ls -1 src/services/trans/*.po | xargs -i msgmerge -U --backup=off --no-wrap {} src/services/trans/translation.pot",
    "apiGen": "ls swager-specs -1 | grep json | xargs -i node ./src/lib/api/generate.js {} && tslint --fix --force src/lib/api/*.ts && tslint --fix --force src/lib/api/*.ts",
    "doc": "typedoc --tsconfig ./tsconfig.json --readme ./README.md --json docs/docs.json --out docs --mode modules ./src"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.15",
    "@fortawesome/free-brands-svg-icons": "^5.7.2",
    "@fortawesome/free-regular-svg-icons": "^5.7.2",
    "@fortawesome/free-solid-svg-icons": "^5.7.2",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^23.3.12",
    "@types/lodash": "^4.14.121",
    "@types/node": "^10.12.18",
    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.30",
    "@types/react-router-dom": "^4.3.0",
    "@types/webpack-env": "^1.13.6",
    "antd": "^3.13.5",
    "awesome-typescript-loader": "^5.2.0",
    "axios": "^0.18.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-import": "^1.11.0",
    "css-loader": "^2.1.0",
    "file-loader": "^3.0.1",
    "gettext-extractor": "^3.4.2",
    "html-webpack-plugin": "^3.2.0",
    "i18next": "^13.1.2",
    "i18next-po-loader": "^1.0.1",
    "image-webpack-loader": "^4.6.0",
    "install": "^0.12.2",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "npm": "^6.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "react-hot-loader": "^4.7.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-actions": "^2.6.1",
    "speed-measure-webpack-plugin": "^1.2.5",
    "style-loader": "^0.23.1",
    "tinycolor2": "^1.4.1",
    "tslint-loader": "^3.6.0",
    "typedoc": "^0.14.1",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.3.3",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "webpack": "^4.29.5",
    "webpack-bundle-analyzer": "^3.0.4",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "antd-jalali-moment": "^3.3.2",
    "connected-react-router": "^4.5.0",
    "git-revision-webpack-plugin": "^3.0.3",
    "strftime": "^0.10.0",
    "swagger-typescript-codegen": "^1.9.0",
    "tslint": "^5.12.1"
  }
}
